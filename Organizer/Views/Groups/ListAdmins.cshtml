@model IEnumerable<Organizer.Models.ApplicationUser>
@{
    ViewBag.Title = "ListAdmins";
}

<h2>ListAdmins</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", "User", new { id = item.Id }, null) |
                @Html.ActionLink("Observe", "Observe", "User", new { id = item.Id }, null)
            </td>
        </tr>
    }

</table>